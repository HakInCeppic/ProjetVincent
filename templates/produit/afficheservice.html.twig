

{% block title %}Mes services{% endblock %}

{% block body %}
<h1>Mes service</h1>

{% for service in services %}


    <div class="alert alert-dismissible alert-danger">
        <button type="button"></button>
    
        <p>{{service.DateCreation}}</p>
        <p>{{service.MonService}}</p>
        <img src="{{service.Logo}}" alt="">
    </div>
{% endfor %}
{% endblock %}
